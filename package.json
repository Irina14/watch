    "browser-sync": "^2.26.7",
    "del": "^5.0.0",
    "eslint": "^6.2.1",
    "eslint-config-htmlacademy": "^0.5.2",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
